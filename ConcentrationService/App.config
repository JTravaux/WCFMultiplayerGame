<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <!-- WCF service configuration (SERVER) -->
  <system.serviceModel>
    <services>
      <service name="ConcentrationLibrary.Concentration">
        <endpoint
          name="ConcentrationService"
          address="net.tcp://fanshawec.ca:13200/ConcentrationLibrary/Concentration"
          binding="netTcpBinding"
          bindingConfiguration="myBindingConfig"
          contract="ConcentrationLibrary.IConcentration" />
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="myBindingConfig">
          <security mode="None"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
  
</configuration>